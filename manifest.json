{
  "manifest_version": 3,
  "name": "MeowBlock: An Undetectable YouTube™ AdBlocker",
  "version": "3.0.3",
  "author": "James Mok",
  "description": "A Free, Undetectable, Open Source YouTube AdBlocker.",
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "permissions": ["scripting"],
  "action":{
    "default_popup": "index.html",
    "default_title": "MeowBlock: An Undetectable YouTube™ AdBlocker"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["script.js"]
    }
  ]
}